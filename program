package exceptionpackage;

public class ExceptionExample {
	
	public static void main(String[] ar)
	{
		int[] num= {1,2,3,4,5};
		try {          //0 1 2 3 4
		for(int i=0;i<=5;i++)
		{
			System.out.println(num[i]);//1 2 3 4 5 
		}
		}catch(ArrayIndexOutOfBoundsException ex)
		{
			System.out.println(ex.getMessage());
		}
	}

}
